{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "app",
  "name": "Log Manager",
  "id": "xformation-logmanager-ui-plugin",
  "info": {
    "description": "Log Manager",
    "author": {
      "name": "Synectiks"
    },
    "logos": {
      "small": "img/asset_logo.svg",
      "large": "img/asset_logo.svg"
    },
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "includes": [
    {
      "type": "page",
      "name": "Dashboard",
      "addToNav": false,
      "defaultNav": false,
      "role": "Admin",
      "path": "/a/%PLUGIN_ID%"
    },
    {
      "type": "page",
      "name": "ContentsPacks",
      "addToNav": false,
      "defaultNav": false,
      "role": "Admin",
      "path": "/a/%PLUGIN_ID%?path=contentspacks"
    },
    {
      "type": "page",
      "name": "Input",
      "addToNav": false,
      "defaultNav": false,
      "role": "Admin",
      "path": "/a/%PLUGIN_ID%?path=input"
    }
  ],
  "dependencies": {
    "grafanaDependency": ">=8.0.0",
    "plugins": []
  }
}